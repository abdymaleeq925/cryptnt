{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Cache-Control": "no-store, no-cache, must-revalidate, private",
          "Pragma": "no-cache",
          "Expires": "0"
        },
        "dest": "server/server.js"
      }
    ],
    "scripts": {
        "build": "echo 'Building...'"
    }
  }